{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": ["plugin:vue/essential", "standard"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "parser": "babel-eslint"
  },
  "plugins": ["vue"],
  "rules": {
    "semi": 0,
    "quotes": 0,
    "space-before-function-paren": 0,
    "no-console": 0,
    "no-path-concat": 0
  }
}
